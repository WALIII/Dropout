function scrapPlot(data)
figure(); 
D = {'r','g','b','c','y','m','k'};
for i = 1:size(data,2);

%  x = 1:size(data{i}',2);
%  hold on;
%  shadedErrorBar(x,data{i}',{@mean,@std},D{i},0);

end

figure();
for i = 1:size(data,2)
 hold on;
%  x = 1:size(data{i}',2);

%  plot(x,mean(data{i},2),D{i});
d = data{i}';
L = size(data{i}',2);
se = std(data{i}')/2;%sqrt(length(data));
mn = mean(data{i}');
col = hsv(size(data,2));
h = fill([1:L L:-1:1],[mn-se fliplr(mn+se)],col(i,:)); alpha(0.5);

plot(mn,'Color',col(i,:));
set(h,'EdgeColor','None');
end



 
 
 
 
 